{
  "name": "mmarod-logcheck",
  "version": "0.0.1",
  "author": "Michael Marod",
  "summary": "Puppet Logcheck module",
  "license": "Apache License, Version 2.0",
  "source": "https://github.com/mmarod/puppet-logcheck.git",
  "project_page": "http://github.com/mmarod/puppet-logcheck",
  "issues_url": "https://github.com/mmarod/puppet-logcheck/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.2.0}
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
